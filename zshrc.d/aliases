function gu() { gitup commit }
function hb() { hub browse }
function pr() { git push origin -u && hub pull-request $* }
function be() { bundle exec "$*"; }
function beg() { be guard "$*"; }
function bek() { be rake "$*"; }
function ber() { be rails "$*"; }
function berc() { ber c "$*"; }
function bers() { ber s "$*"; }

